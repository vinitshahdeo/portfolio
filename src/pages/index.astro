---
import { Image } from "astro:assets";
import Layout from "../layouts/Layout.astro";
import { homePageContent, identity } from "../config";
import SocialLink from "../components/SocialLink.astro";
import Link from "../components/Link.astro";
import { Icon } from "astro-icon/components";
---

<Layout seo={homePageContent.seo}>
  <section class="mt-10 max-w-2xl mx-auto px-6">
    <h1 class="text-3xl mb-1">{identity.name}</h1>
    <p class="opacity-60 mb-10 subtitle">
      {homePageContent.role}<span
        class="lazy-badge inline-flex items-center bg-[#2a2a2a] text-[#ffe895] text-xs font-semibold ml-2 px-2.5 py-0.5 rounded-full border border-[#3a3a3a]"
        ><span
          class="inline-block w-2 h-2 rounded-full bg-[#00abda] mr-1.5 pulse-dot"
        ></span>
        Hiring
      </span>
    </p>
    <div class="flex flex-row flex-wrap gap-6 mb-8">
      <Image
        src="/img/vinitshahdeo-home.png"
        alt={identity.name}
        width={100}
        height={100}
        loading="lazy"
        decoding="async"
        quality={100}
        densities={[1.5, 2]}
        class="rounded-lg border border-[#3a3a3a] border-2 border-opacity-80"
      />
      <div class="group flex flex-col justify-center gap-2 w-fit social-bar">
        {
          homePageContent.homeSocialLinks.map((link) => (
            <SocialLink
              title={link.title}
              url={link.url}
              icon={link.icon}
